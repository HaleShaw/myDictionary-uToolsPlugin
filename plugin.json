{
    "pluginName": "词典",
    "version": "1.0.2",
    "description": "中英词典插件",
    "author": "HaleShaw",
    "homepage": "https://github.com/HaleShaw/uTools-Dictionary",
    "main": "index.html",
    "logo": "logo.png",
    "features": [
        {
            "code": "search_word",
            "explain": "中英文翻译，音标朗读，基于有道移动版网页。",
            "cmds": [
                "词典",
                "dictionary",
                "音标",
                "phonetic",
                "翻译",
                "d",
                {
                    "type": "regex",
                    "label": "Dictionary",
                    "match": "/^[\u4e00-\u9fa5a-zA-Z][\u4e00-\u9fa5a-zA-Z0-9' ]/",
                    "minLength": 1
                }
            ]
        }
    ]
}
